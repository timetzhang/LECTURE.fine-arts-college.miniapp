/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.nav{
  flex-flow:row;
  flex-direction: row;
  display: flex;
  background-color: rgb(240,240,240);
  font-size:12px;
  padding:5px;
  width:100%;

  position: fixed;
  left: 0;
  bottom:0;
}

.nav-btn{
  width: 25%;
  text-align:center;
}

.nav-img{
  width:20px;
  height:20px;
}